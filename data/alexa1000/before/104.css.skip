}
.leftNav li
{
	display:block;
	padding:0px 0px 0px 0px;
	text-decoration:none;
	position:relative;
	height:90px;

}
.leftNav li:first-child a,
.leftNav li:first-child .imageAlt span
{
	border-radius:0px 10px 10px 0px;
}
.leftNav .last a,
.leftNav .last .imageAlt span
{
	border-radius:0px 10px 10px 0px;
}
.leftNav li .imageAlt
{
	z-index:2;
	width:37px;
	height:90px;
	background-color:#373737;
